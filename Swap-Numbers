#include <stdio.h>
int main()
{
    double a, b;
    printf("Enter the number a: ");
    scanf("%lf", &a);
    printf("Enter the number b: ");
    scanf("%lf", &b);

    // Start of swapping program

    a = a - b;
    b = a + b;
    a = b - a;

    printf("After swapping, a = %.2lf\n", a);
    printf("After swapping, b = %.2lf", b);
    return 0;
}
